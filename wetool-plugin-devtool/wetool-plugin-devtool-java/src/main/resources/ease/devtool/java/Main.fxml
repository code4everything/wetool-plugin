<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.code4everything.wetool.plugin.devtool.java.controller.MainController">
    <HBox prefWidth="200.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <TextField fx:id="class4Javap" editable="false" promptText="CLASS文件" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </TextField>
        <TextField fx:id="targetFile" promptText="目标文件" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets left="10.0" right="10.0"/>
            </HBox.margin>
        </TextField>
        <Button mnemonicParsing="false" onAction="#chooseTargetFile" text="浏览" HBox.hgrow="NEVER"/>
        <Button mnemonicParsing="false" onAction="#javap" text="反汇编" HBox.hgrow="NEVER">
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <Separator disable="true" prefWidth="200.0" VBox.vgrow="NEVER">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
    </Separator>
    <HBox prefWidth="200.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <TextField fx:id="classes4BeanJson" promptText="CLASSES目录">
            <HBox.margin>
                <Insets right="10.0"/>
            </HBox.margin>
        </TextField>
        <TextField fx:id="class4BeanJson" promptText="CLASS文件" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets right="10.0"/>
            </HBox.margin>
        </TextField>
        <Button mnemonicParsing="false" onAction="#convertBean2Json" text="BEAN转JSON"/>
    </HBox>
</VBox>
