<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.code4everything.wetool.plugin.dbops.controller.MainController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox prefWidth="200.0" VBox.vgrow="NEVER">
         <children>
            <Button onAction="#execute" mnemonicParsing="false" text="执行" HBox.hgrow="NEVER" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Separator prefWidth="200.0" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Separator>
      <TextArea fx:id="sqlText" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </TextArea>
   </children>
</VBox>
